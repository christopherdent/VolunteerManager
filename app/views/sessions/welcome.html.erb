<h1> Welcome to Volunteer Manager </h1>
  <p>

  <% if logged_in? %>
    Hi, <%= current_user.first_name %>.  What would you like to do?
 <% end %>
  </p>
<% if !logged_in? %>
    <%= link_to "Login", login_path %>
    OR
    <%= link_to "Signup", signup_path %>
  </p>
  <%else%>

  <ul>
    <li><%= link_to 'Add a Volunteer', new_volunteer_path %></li>
    <li><%= link_to 'View All Volunteers', volunteers_path %></li>
    <li><%= link_to 'Create a Group', new_group_path %></li>
    <li><%= link_to 'View All Groups', groups_path %></li>
  </ul>

<%end%>
