<h1>Sign Up</h1>

<%= form_for @user do |f| %>

<%= f.label :first_name %>
<%= f.text_field :first_name %>
<br>
<%= f.label :last_name %>
<%= f.text_field :last_name %>
<br>
<%= f.label :email %>
<%= f.email_field :email %>
<br>
<%= f.label :username %>
<%= f.text_field :username %>
<br>
<%= f.label :password %>
<%= f.password_field :password %>
<br>
<%= f.submit %>
<br>
<% end %>

<p>
Already have an account?  <%= link_to 'Login.', login_path %>
